{
  "version": "1.0.0",
  "github": {
    "public": {
      "api_type": "public",
      "api_url": "https://api.github.com/",
      "auth_token": "$TOKEN",
      "labels": ["webhooks"],
      "issue_state": "all",
      "organizations": [{
        "paypal": [
          "PayPal-REST-API-issues"
        ]
      }]
    }
  },
  "stackoverflow": {
    "api_key": "NA",
    "api_url": "https://api.stackexchange.com/",
    "order": "desc",
    "sort": "activity",
    "tag": "paypal",
    "intitle": "webhooks",
    "site": "stackoverflow",
    "access_token": "$ACCESS_TOKEN",
    "key": "$KEY"
  },
  "twitter": {
    "consumer_key": "$CONS_KEY",
    "consumer_secret": "$CONS_SECRET",
    "access_token_key": "$ACCESS_TOKEN",
    "access_token_secret": "$ACCESS_SECRET",
    "default_user":"paypaldev",
    "default_tweet_count":"25"
  },
  "admin_config" : {
    "slack" : {
      "slack_token_encrypted" : "$TOKEN"
    }
  }
}
